{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run -C examples dev",
    "build": "tsx ./packages/vite.config.mts",
    "build:cmp": "pnpm run build cmp",
    "build:use": "pnpm run  build use",
    "build:dir": "pnpm run build dir",
    "test": "vitest -c ./packages/vitest.config.mts",
    "test:cg": "vitest --coverage -c ./packages/vitest.config.mts",
    "test:cmp": "pnpm run test ./packages/tests/components/*.test.ts",
    "test:use": "pnpm run test ./packages/tests/use/*.test.ts",
    "lint": "eslint --fix --cache --cache-location .cache/.eslintcache",
    "lint:all": "pnpm run lint .",
    "type-check": "vue-tsc --build",
    "type-check:force": "pnpm run type-check --force",
    "format": "prettier --write '!(package|pnpm-lock).*' --ignore-unknown",
    "format:all": "pnpm run format .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy:docs": "pnpm run -C docs deploy",
    "prepare": "husky"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@floating-ui/vue": "^1.1.5",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-onboarding": "^8.6.9",
    "@storybook/blocks": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@storybook/vue3": "^8.6.9",
    "@storybook/vue3-vite": "^8.6.9",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/runtime-core": "^3.4.27",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.9.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "storybook": "^8.6.9",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "1.2.2",
    "vitest": "^1.6.0",
    "vue": "^3.5.12",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.19"
  }
}
